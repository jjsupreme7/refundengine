# OpenAI API Key (Required for GPT-4o Vision and embeddings)
OPENAI_API_KEY=sk-your-openai-key-here

# Supabase Configuration (Required for vector database and knowledge base)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Database Configuration (for direct PostgreSQL access)
SUPABASE_DB_HOST=db.your-project.supabase.co
SUPABASE_DB_USER=postgres
SUPABASE_DB_PASSWORD=your-db-password-here
SUPABASE_DB_NAME=postgres
SUPABASE_DB_PORT=5432

# Optional: Anthropic API (if using Claude instead of GPT)
ANTHROPIC_API_KEY=your-anthropic-key-here
