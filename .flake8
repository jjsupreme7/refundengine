[flake8]
max-line-length = 88
extend-ignore = E203, W503
per-file-ignores =
    __init__.py:F401
    */migrations/*:E501
    */deprecated/*:E501,F401,F841
exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    build,
    dist,
    *.egg-info,
    .tox,
    .pytest_cache,
    node_modules
